{
   "manifest_version": 2,
   "name": "Czo Slick Theme",
   "version": "0.5",
   "default_locale": "en",
   "description": "Czo Slick Theme",
   "theme": {
      "colors": {
         "bookmark_text": [ 0, 0, 0 ],
         "frame": [ 32, 32, 32 ],
         "ntp_background": [ 230, 230, 230 ],
         "ntp_link": [ 0, 0, 120 ],
         "ntp_section": [ 210, 210, 210, 1 ],
         "ntp_section_link": [ 0, 0, 120 ],
         "ntp_section_text": [ 0, 0, 0 ],
         "ntp_text": [ 0, 0, 0 ],
         "tab_background_text": [ 0, 0, 0 ],
         "tab_text": [ 0, 0, 0 ],
         "toolbar": [ 230, 230, 230 ]
      },
      "images": {
         "theme_button_background": "images/button_background",
         "theme_frame": "images/Frame.png",
         "theme_toolbar": "images/theme_toolbar.png"
      },
      "properties": {
         "ntp_background_alignment": "bottom\ncenter",
         "ntp_background_repeat": "no-repeat"
      },
      "tints": {
         "buttons": [ 0.6, 0, 0.5 ]
      }
   }
}
